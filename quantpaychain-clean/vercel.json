{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd apps/web && yarn build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "yarn install",
  "devCommand": "cd apps/web && yarn dev",
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
  "build": {
    "env": {
      "NEXT_PHASE": "phase-production-build"
    }
  }
}
